<div class="new-tour-form">
  <%= bootstrap_form_for [:vendor, @tour], :url => user_vendor_tour_path(:vendor, @vendor, @tour) do |f| %>
    <%= f.text_field :name, label: "Name:" %>
    <%= f.text_field :description, label: "Description:" %>
    <%= f.text_field :price, label: "Price:" %>
    <%= f.submit "Create Tour", class:"btn btn-sign-in" %>
  <% end %>
</div>
